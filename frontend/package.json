{
  "name": "calorie-track-mini-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "dev:5173": "vite --host 0.0.0.0 --port 5173",
    "build": "tsc -b && vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview --port 5173 --host 0.0.0.0",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 10",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "i18n:validate": "node src/i18n/validate-keys.mjs",
    "test": "node test-ci.js",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:ui-ux": "playwright test tests/e2e/ui-ux-validation.spec.ts tests/e2e/accessibility.spec.ts",
    "test:accessibility": "playwright test tests/e2e/accessibility.spec.ts",
    "test:performance": "playwright test tests/e2e/ui-ux-validation.spec.ts --grep @performance",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:contracts": "vitest run --config vitest.contracts.config.ts",
    "test:components": "vitest run src/components",
    "test:services": "vitest run src/services",
    "test:all": "npm run test && npm run test:e2e",
    "check": "npm run type-check && npm run lint && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "clean": "rm -rf dist node_modules/.vite .vite",
    "clean:all": "npm run clean && rm -rf node_modules",
    "deps:update": "npm update && npm audit",
    "deps:check": "npm outdated",
    "analyze": "npm run build && npx bundle-analyzer dist/assets/*.js",
    "serve:local": "npm run build && npm run preview",
    "docker:build": "docker build -t calorie-track-frontend .",
    "docker:run": "docker run -p 3000:3000 calorie-track-frontend",
    "setup": "npm install && npm run check",
    "start": "npm run dev"
  },
  "dependencies": {
    "@telegram-apps/sdk": "^3.11.5",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@vitest/ui": "^3.2.4",
    "axios": "^1.7.7",
    "i18next": "^25.5.2",
    "react": "^19.1.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.3.0",
    "swiper": "^11.2.10",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.36.0",
    "@jest/globals": "^30.1.2",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "playwright": "^1.47.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}
